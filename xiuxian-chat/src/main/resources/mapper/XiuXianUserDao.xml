<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xiuxian.chat.dao.XiuXianUserDao">



    <select id="getByXiuXianUserId" resultType="com.xiuxian.chat.entity.XiuXianUserEntity">
        select * from tb_user where xiuxian_user_id = #{xiuxianUserId}
    </select>

    <select id="getByMobile" resultType="com.xiuxian.chat.entity.XiuXianUserEntity">
        select * from tb_user where mobile = #{mobile}
    </select>


    <select id="getById" resultType="com.xiuxian.chat.entity.XiuXianUserEntity">
        select * from tb_user where id = #{id}
    </select>

</mapper>