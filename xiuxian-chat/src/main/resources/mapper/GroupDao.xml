<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xiuxian.chat.dao.GroupDao">


    <select id="getByXiuXianGroup" resultType="com.xiuxian.chat.entity.GroupEntity">
        select *
        from tb_group
        where xiuxian_group_id = #{xiuxianGroupId}
    </select>


</mapper>